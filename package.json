{
  "name": "placemark",
  "version": "1.0.0",
  "description": "Placemark POI Service v1",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url sass/style.scss public/css/style.css",
    "css-watch": "npm run css-build -- --watch",
    "test": "./node_modules/mocha/bin/mocha --ui tdd test/**/*.js",
    "dev": "nodemon --config .nodemonrc.json server.js --ignore 'models/json/*.json'",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huckcity/placemark.git"
  },
  "author": "Adam Gibbons",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/huckcity/placemark/issues"
  },
  "homepage": "https://github.com/huckcity/placemark#readme",
  "dependencies": {
    "@hapi/basic": "^6.0.0",
    "@hapi/boom": "^9.1.4",
    "@hapi/cookie": "^11.0.2",
    "@hapi/hapi": "^20.2.1",
    "@hapi/inert": "^6.0.5",
    "@hapi/vision": "^6.1.0",
    "axios": "^0.26.0",
    "dotenv": "^16.0.0",
    "handlebars": "^4.7.7",
    "hapi-swagger": "^14.2.5",
    "joi": "^17.6.0",
    "lowdb": "^3.0.0",
    "mais-mongoose-seeder": "^1.0.7",
    "mongoose": "^6.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "bulma": "^0.9.3",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "mocha": "^9.2.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15"
  }
}
