{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"],
        "no-underscore-dangle": "off",
        "import/extensions": "off",
        "no-undef": "off",
        "import/prefer-default-export": "off",
        "object-curly-newline": "off",
        "no-console": "off",
        "no-param-reassign": "off"        
    }
}
